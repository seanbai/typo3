{
  "name": "mschwemer/rte_ckeditor_wordcount",
  "type": "typo3-cms-extension",
  "description": "Adds the word-count add-on to the CKEditor in TYPO3.",
  "homepage": "https://www.typo3worx.eu",
  "license": ["GPL-2.0+"],
  "require": {
    "typo3/cms": "^8.7.0"
  },
  "replace": {
    "rte_ckeditor_wordcount": "self.version",
    "typo3-ter/rte_ckeditor_wordcount": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
